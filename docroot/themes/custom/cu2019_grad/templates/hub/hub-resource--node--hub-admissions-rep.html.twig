{#
/**
 * @file
 * Default theme file to display a hub_resource item.
 *
 * Available variables:
 * - content: hub resource content.
 *
 * @see template_preprocess_hub_resource()
 *
 * @ingroup themeable
 */
#}
{%
  set classes = [
    'hub-resource',
    'hub-resource--type-' ~ resource_type|clean_class,
  ]
%}
<article{{ attributes.addClass(classes) }}>
  {% if content %}
    <h3>Still have questions?</h3>
    <div>
      Reach out to a {{ content.field_hub_adm_rep_honorific|render|striptags }}
    </div>
    <div>
      {{ content.field_hub_adm_rep_name }}
      {{ content.field_hub_adm_rep_email }}
      {{ content.field_hub_adm_rep_phone }}
      {{ content.field_hub_adm_rep_image }}
    </div>
  {% endif %}
</article>
