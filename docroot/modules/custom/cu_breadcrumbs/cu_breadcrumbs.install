<?php
use Drupal\cu_breadcrumbs\Breadcrumb\CUBreadcrumbModel;
/**
* Implements hook_schema().
*
* Defines the database tables used by cu_breadcrumbs module.
*
* @see hook_schema()
*
* @ingroup cu_breadcrumbs
*/
function cu_breadcrumbs_schema() {
  return CUBreadcrumbModel::getSchema();
}

function cu_breadcrumbs_uninstall() {
  drupal_uninstall_schema(CUBreadcrumbModel::getTable());
}
